# LangChain 기반 코드 생성 추천기 환경 변수 예시 파일
# 이 파일을 복사하여 .env 파일로 저장한 후 실제 값을 입력하세요

# LangSmith 관련 설정 (선택 사항)
# LangChain 프로젝트 모니터링 및 디버깅을 위한 도구입니다
LANGSMITH_TRACING=true
LANGSMITH_ENDPOINT="https://api.smith.langchain.com"
LANGSMITH_API_KEY="your_langsmith_api_key_here"
LANGSMITH_PROJECT="YourProjectName"

# OpenAI API 설정 (필수)
# 코드 생성 추천기가 작동하려면 OpenAI API 키가 필요합니다
# https://platform.openai.com 에서 API 키를 발급받을 수 있습니다
OPENAI_API_KEY=your_openai_api_key_here

# 서버 설정 (선택 사항)
# 기본값은 3000입니다

PORT=3000
SESSION_SECRET=your_session_secret

# MongoDB 설정
MONGODB_URI="몽고 DB 주소"

# GitHub OAuth 설정
GITHUB_CLIENT_ID="깃허브 클라이언트 ID"
GITHUB_CLIENT_SECRET="깃허브 클라이언트 시크릿"
GITHUB_CALLBACK_URL="깃허브 콜백 URL"

# 텍스트 청크 설정 ( 선택 사항 )
# 청크 크기와 중첩 크기를 설정합니다
CHUNK_SIZE=1000
CHUNK_OVERLAP=200